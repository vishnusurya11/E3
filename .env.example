# E3 ComfyUI Agent - Environment Configuration Example
# Copy this file to .env and update with your values

# Environment Configuration
# Set to 'alpha', 'prod', 'dev', or leave unset for default
# This will load environment-specific config files (e.g., global_config.alpha.yaml)
E3_ENV=alpha

# AI Model API Keys
OPENAI_API_KEY=your_openai_api_key_here
GOOGLE_API_KEY=your_google_api_key_here
GEMINI_API_KEY=your_gemini_api_key_here
ANTHROPIC_API_KEY=your_anthropic_api_key_here
CLAUDE_API_KEY=your_claude_api_key_here

# Default AI Model Configuration
DEFAULT_MODEL=gpt-4o
DEFAULT_TEMPERATURE=0.7

# ComfyUI Settings
COMFYUI_HOST=127.0.0.1
COMFYUI_PORT=8188
COMFYUI_TIMEOUT=300

# Database Settings
DATABASE_PATH=database/comfyui_agent.db

# Queue Settings
DEFAULT_PRIORITY=50
RETRY_LIMIT=2
POLL_INTERVAL_MS=1000

# Paths
JOBS_PROCESSING_PATH=jobs/processing
JOBS_FINISHED_PATH=jobs/finished
WORKFLOWS_PATH=workflows

# UI Settings
UI_HOST=127.0.0.1
UI_PORT=8080

# Logging
LOG_LEVEL=INFO
LOG_FILE=logs/comfyui_agent.log

# YouTube OAuth2 Credentials (for video upload)
# Get these from Google Cloud Console: https://console.cloud.google.com/apis/credentials
# 1. Create OAuth client ID (Desktop application)
# 2. Copy client_id and client_secret values
YOUTUBE_API_KEY=your_youtube_api_key_here
YOUTUBE_CLIENT_ID=your_client_id.apps.googleusercontent.com
YOUTUBE_CLIENT_SECRET=GOCSPX-your_client_secret_here
YOUTUBE_CHANNEL_ID=your_channel_id_here